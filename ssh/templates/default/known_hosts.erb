<% @nodes.each do |n| -%>
<% next unless n[:uptime] %>
<%= n['hostname'] %>,<%= n['ipaddress'] %> ssh-rsa <%= n['keys']['ssh']['host_rsa_public'] %>
<% end -%>
<% @known_ssh_keys.each |name, conf| -%>
<%= name %>,<%= conf['ipaddress'] %> ssh-rsa <%= conf['key'] %>
<% end -%>